import time
from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys

# 配置 WebDriver
driver = webdriver.Chrome()  # 确保已安装 ChromeDriver 并配置到 PATH
driver.maximize_window()

try:
    # 打开高德地图官网
    driver.get("https://www.amap.com/")

    # 等待页面加载
    time.sleep(3)

    # 输入起点
    start_input = driver.find_element(By.ID, "searchInput")
    start_input.send_keys("起点地址")  # 替换为实际起点
    start_input.send_keys(Keys.ENTER)
    time.sleep(2)

    # 输入终点
    end_input = driver.find_element(By.ID, "searchInput")
    end_input.clear()
    end_input.send_keys("终点地址")  # 替换为实际终点
    end_input.send_keys(Keys.ENTER)
    time.sleep(2)

    # 点击导航按钮
    nav_button = driver.find_element(By.CLASS_NAME, "route-plan-button")
    nav_button.click()
    time.sleep(5)

    # 验证导航结果
    directions = driver.find_elements(By.CLASS_NAME, "route-item")
    if directions:
        print("导航测试成功，找到路线。")
    else:
        print("导航测试失败，未找到路线。")

finally:
    # 关闭浏览器
    driver.quit()


    ● 围栏信息
    ○ JT/T808—2013的 表 28
        ■ AlarmMarkOfTable28{
            ● mLocationType=
                ○ 1：圆形区域
                ○ 2：矩形区域
                ○ 3：多边形区域
            ● mAreaId=
                ○ 配置的围栏Id
● 进出围栏
    ○ JT/T808—2013的 表 29
        ■ AlarmMarkOfTable29{
            ● mLocationType=
                ○ 1：圆型区域
                ○ 2：矩形区域
                ○ 3：多边形区域
            ● mAreaId=
                ○ 配置的围栏Id
            ● mDirection=
                ○ 0：进围栏
                ○ 1：出围栏
● 超速
    ○ 808-2013 表24
        ■ ,mOverSpeed1=
            ● 0未超速
            ● 1超速
    ○ JT/T808—2011的 表 18
        ■ mAlarmMark1=
            ● 0未超速
            ● 1超速